*,:after,:before{box-sizing:border-box}html{-moz-text-size-adjust:none;-webkit-text-size-adjust:none;text-size-adjust:none;font-size:16px;height:100%;overflow-y:auto;scroll-behavior:smooth}h1,h2,h3,h4{margin-bottom:0;text-wrap:balance;line-height:1.1}ol[role=list],ul[role=list]{list-style:none}body{font-weight:400;line-height:1.5;min-height:100vh}a:not([class]){color:currentColor;-webkit-text-decoration-skip:ink;text-decoration-skip-ink:auto}code,kbd,pre,samp{font-family:monospace,monospace}b,strong{font-weight:bolder}img,picture{display:block;max-width:100%}button,input,select,textarea{font:inherit}textarea:not([rows]){min-height:10em}:target{scroll-margin-block:1rem}#footer-main,#header-main{text-align:center}.h1,.h3,h1,h3{text-align:left}.h2,h2{text-align:right}.h2>a:hover,.site-logo:hover,h2>a:hover{color:#1a95e0}#footer-main ul,#header-main ul,.list-meta,pre{margin:0}#footer-main,#header-main,main>*{border:1px solid}.h2,h2{border-bottom:1px solid}#footer-main ul,#header-main ul,#tagcloud,.list-meta,.list-page{list-style:none;padding:0}#footer-main ul,#header-main ul,#tagcloud,.list-meta{flex-wrap:wrap}#tagcloud,.container-meta{color:#647382;font-size:.8rem}@media(prefers-color-scheme:dark){#tagcloud,.container-meta{color:#d4d9dd}}#footer-main ul,#header-main ul,#tagcloud,.list-meta{display:flex}article{flex-grow:1}#footer-main ul,#header-main ul{justify-content:center}.list-meta{gap:.5em}#tagcloud{gap:1em}body,main{display:grid;gap:1rem}body{font-family:ui-monospace,cascadia code,source code pro,Menlo,Consolas,dejavu sans mono,monospace;grid-template-rows:auto 1fr auto}@media(min-width:50em){body{margin:1rem auto;max-width:50rem}}@media(prefers-color-scheme:dark){body{background-color:#000;color:#fff}}main>*{padding:0 1rem}main>article{overflow-x:auto}#skiplink{border:0;clip:rect(0 0 0 0);background-color:gray;color:#fff;height:1px;margin:-1px;overflow:hidden;padding:1em 2em;position:absolute;-webkit-text-decoration:none;text-decoration:none;text-transform:uppercase;white-space:nowrap;width:1px}#skiplink:active,#skiplink:focus{clip:auto;height:auto;margin:0;overflow:visible;position:static;white-space:normal;width:auto}#footer-main,#header-main{padding:.5rem 0}#footer-main ul,#header-main ul{font-family:ui-monospace,cascadia code,source code pro,Menlo,Consolas,dejavu sans mono,monospace;gap:.5em 1em}#footer-main{content-visibility:auto;contain-intrinsic-size:auto 50px auto 300px}.site-logo{display:block;font-family:ui-monospace,cascadia code,source code pro,Menlo,Consolas,dejavu sans mono,monospace;font-size:1.8rem;-webkit-text-decoration:none;text-decoration:none}article{content-visibility:auto;contain-intrinsic-size:auto 800px auto 300px}.h1,h1{border-bottom:none;font-size:2rem;padding-bottom:0}.h2,h2{font-size:1.5rem;padding-bottom:.5rem}.h2>a,h2>a{-webkit-text-decoration:none;text-decoration:none}.h3,h3{border-bottom:none;padding-bottom:0}@media(min-width:50em){p{-webkit-hyphens:auto;hyphens:auto;text-align:justify}}a{color:currentColor;text-decoration-thickness:.06em;text-underline-offset:.15em}a:focus,a:hover{-webkit-text-decoration:none;text-decoration:none}.list-page li{margin-bottom:1.25rem}.deck{font-style:italic}#tagcloud{font-size:1rem}pre{overflow:auto;padding:0 .5em}pre code{background-color:inherit}code{background-color:#d4d9dd;border-radius:.2em;padding:0 .2em}@media(prefers-color-scheme:dark){code{background-color:gray}}